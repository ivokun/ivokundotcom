{
  "name": "@ivokundotcom/cms",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "ivokun CMS - Custom headless CMS built with Effect TS, Kysely, and Bun",
  "scripts": {
    "dev": "bun run --watch src/server.ts & vite dev --config vite.config.ts",
    "dev:server": "bun run --watch src/server.ts",
    "dev:spa": "vite dev --config vite.config.ts",
    "build": "bun run scripts/build-binary.ts",
    "build:quick": "bun build src/server.ts --compile --outfile dist/cms --external @effect/cluster --external sharp",
    "build:spa": "vite build --config vite.config.ts",
    "build:linux": "bun run scripts/build-binary.ts linux-x64",
    "build:all": "bun run scripts/build-binary.ts --all",
    "start": "./dist/cms",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "db:new": "dbmate -d ./db/migrations new",
    "db:up": "dbmate -d ./db/migrations up",
    "db:down": "dbmate -d ./db/migrations down",
    "db:status": "dbmate -d ./db/migrations status",
    "db:dump": "dbmate -d ./db/migrations dump",
    "seed:admin": "bun run scripts/seed-admin.ts",
    "migrate:strapi": "bun run scripts/migrate-strapi.ts",
    "test": "bun test"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.994.0",
    "@aws-sdk/s3-request-presigner": "^3.994.0",
    "@effect/cluster": "0.55.0",
    "@effect/platform": "0.93.6",
    "@effect/platform-bun": "0.86.0",
    "@node-rs/argon2": "2.0.2",
    "@paralleldrive/cuid2": "2.2.2",
    "effect": "3.19.8",
    "kysely": "0.27.4",
    "pg": "8.13.1",
    "sharp": "^0.34.5",
    "slugify": "^1.6.6"
  },
  "devDependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.0",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-dialog": "^1.1.0",
    "@radix-ui/react-dropdown-menu": "^2.1.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-select": "^2.1.0",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.0",
    "@tanstack/react-query": "^5.65.0",
    "@tanstack/react-router": "^1.120.0",
    "@tanstack/router-devtools": "^1.120.0",
    "@tiptap/core": "^3.15.1",
    "@tiptap/extension-image": "^3.15.1",
    "@tiptap/extension-link": "^3.15.1",
    "@tiptap/extension-placeholder": "^3.15.1",
    "@tiptap/react": "^2.11.0",
    "@tiptap/starter-kit": "^3.15.1",
    "@types/bun": "1.1.14",
    "@types/pg": "8.11.10",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "10.4.20",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint": "9.17.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "lucide-react": "^0.475.0",
    "postcss": "8.4.49",
    "prettier": "3.4.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sonner": "^2.0.0",
    "tailwind-merge": "^3.0.0",
    "tailwindcss": "3.4.17",
    "typescript": "5.7.2",
    "typescript-eslint": "^8.56.0",
    "vite": "6.0.7"
  },
  "engines": {
    "node": ">=22.17.0",
    "bun": ">=1.1.38"
  }
}
